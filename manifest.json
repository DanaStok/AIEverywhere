{
  "manifest_version": 3,
  "name": "AIEverywhere",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://api.openai.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "./icons/download-icon-chrome+icon-1320192123565798996_128.icns",
    "128": "./icons/download-icon-chrome+icon-1320192123565798996_128.icns"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
